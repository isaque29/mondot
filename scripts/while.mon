unit Program
{
    on MdReload -> ()
        -- A while loop
        local n = sub(0, 1000000);

        while (1)
            n = add(n, 1);
            io.write(add(n, ' '));

            if (n)
            else
                io.writeln('\nok');
                io.flush();
                return 0;
            end
        end
    end
}
